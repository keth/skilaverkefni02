<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="processContext" class="is.ruframework.process.RuProcessContext">

        <property name="processName">
            <value>PinImportProcess</value>
        </property>
        <property name="importURL">
            <value>http://feeds.bbci.co.uk/news/technology/rss.xml</value>
        </property>
        <property name="processClass">
            <value>is.ru.honn.rupin.process.PinImportProcess</value>
        </property>
    </bean>

    <bean id="userBean" class="is.ru.honn.rupin.domain.User">
        <property name="username">
            <value>User01</value>
        </property>
        <property name="firstName">
            <value>Hattur</value>
        </property>
        <property name="lastName">
            <value>Fattur</value>
        </property>
        <property name="email">
            <value>hattu@fattur.is</value>
        </property>
        <property name="password">
            <value>password123</value>
        </property>
        <property name="gender">
            <value>MALE</value>
        </property>
    </bean>

    <bean id="boardBean" class="is.ru.honn.rupin.domain.Board">
        <property name="name">
            <value>Board01</value>
        </property>
        <property name="category">
            <value>Technology</value>
        </property>
    </bean>


</beans>